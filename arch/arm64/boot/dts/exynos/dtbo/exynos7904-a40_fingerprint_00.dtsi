size = <256>;	interrupts = <0 256 0>;	dma-mode;	dmas = <&pdma0 21		&pdma0 20>;	gfspi-spi@0 {		compatible = "goodix,fingerprint";		reg = <0>;		spi-max-frequency = <16000000>;		pinctrl-names = "default", "pins_poweron", "pins_poweroff"						, "pins_poweron_tz", "pins_poweroff_tz";		pinctrl-0 = <&btp_irq_sleep &btp_reset &btp_ldo>;		pinctrl-1 = <&btp_irq &spi1_clk &spi1_cs &spi1_miso &spi1_mosi>;		pinctrl-2 = <&btp_irq_sleep &spi1_bus_inactive>;		pinctrl-3 = <&btp_irq>;		pinctrl-4 = <&btp_irq_sleep>;		gpio-controller;		#gpio-cells = <2>;		goodix,gpio_reset = <&gpg3 2 0x00>;		goodix,gpio_irq = <&gpa0 5 0x00>;		goodix,gpio_pwr = <&gpg3 3 0x00>;		goodix,chip_id = "GW36H3";		goodix,orient = <0>;		controller-data {			samsung,spi-feedback-delay = <0>;			samsung,spi-chip-select-mode = <0>;		};	};};