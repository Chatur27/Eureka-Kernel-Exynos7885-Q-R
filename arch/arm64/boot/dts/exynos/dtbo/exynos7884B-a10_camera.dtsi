#include <dt-bindings/camera/fimc_is.h>

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {

			/* REAR CAMERA */
			fimc_is_module_3l6: fimc-is_sensor_3l6@5A {
				compatible = "samsung,sensor-module-3l6";
				pinctrl-names = "pin0", "pin1", "pin2", "release";

				pinctrl-0 = <>;
				pinctrl-1 = <&fimc_is_mclk2_out>;
				pinctrl-2 = <&fimc_is_mclk2_fn>;
				pinctrl-3 = <>;

				//use_pdaf; /* when USE_AP_PDAF is enabled, use for pdaf sensor */

				position = <0>;					/* Rear:0. Front:1 */
				id = <0>; 					/* bns_id */
				mclk_ch = <2>;					/* XCI0_MCLK */
				sensor_i2c_ch = <1>;				/* HSI2C1 */

				gpio_mclk = <&gpc0 2 0x1>;			/* RCAM1_RCAM2_MCLK - XCI0_MCLK */
				gpio_reset = <&gpg3 4 0x1>;			/* RCAM1_RST_N - XGPIO24 */
				gpio_camaf_2p8_en = <&gpg1 5 0x1>;		/* RCAM1_AFVDD_2P8_EN*/
				gpio_cam_2p8_en = <&gpg2 1 0x1>;
				gpio_cam_core_en = <&gpg1 4 0x1>;
				gpio_camio_1p8_en = <&gpg1 7 0x1>;
				shared_camio_1p8;
				status = "okay";

				af {
					product_name = <19>;			/* ACTUATOR_NAME_DW9808 */
					i2c_addr = <0x18>;
					i2c_ch = <3>;
				};

				flash {
					product_name = <11>; 			/* FLADRV_NAME_DRV_FLASH_GPIO : S2MU005 */
					flash_first_gpio = <2>;			/* DICO not use first, second gpio value */
					flash_second_gpio = <1>;
				};

			};
			/* FRONT CAMERA */
			fimc_is_sensor_5e9: fimc-is_sensor_5e9@5A {
				compatible = "samsung,sensor-module-5e9";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <&fimc_is_mclk1_out>;
				pinctrl-1 = <&fimc_is_mclk1_out>;
				pinctrl-2 = <&fimc_is_mclk1_fn>;
				pinctrl-3 = <>;

				position = <1>;		/* Rear:0. Front:1 */
				id = <1>; 			/* bns_id */
				mclk_ch = <1>;

				gpio_mclk = <&gpc0 1 0x1>;
				gpio_reset = <&gpg3 5 0x1>;		/* sensor reset */
				gpio_camio_1p8_en = <&gpg1 7 0x1>; 	/* CAM_IO_LDO_EN */
				gpio_cam_core_en = <&gpg2 4 0x1>; 	/* VTCAM_CORE_1P05_EN */
				gpio_cam_a2p8_en = <&gpg3 0 0x1>;	/* VTCAM_A2P8_EN */
				shared_camio_1p8;
				status = "okay";
			};

			/* FLASH */
			fimc_is_flash_gpio: fimc-is-flash_gpio@0 {
				compatible = "samsung,sensor-flash-gpio";
				id = <0>; 			/* Rear  */
				status = "okay";

				flash-gpio = <&gpg2 7 0>;	/* CAM_FLASH_EN - XGPIO19 */
				torch-gpio = <&gpg0 1 0>;	/* CAM_TORCH_EN - XGPIO26 */
			};

		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

