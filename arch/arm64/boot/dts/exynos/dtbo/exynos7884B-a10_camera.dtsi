/* fimc_is_sensor0 - XMIPI_CSI4LA */
&fimc_is_sensor0 {
	scenario = <SENSOR_SCENARIO_NORMAL>;		/* Normal, Vision, OIS etc */
	id = <0>;					/* SENSOR_POSITION */
	csi_ch = <1>;					/* CSI Channel Number */
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	is_flite = <0>;
	status = "okay";

	//use_ssvc1_internal;				/* Internal Virual Channel */
};

/* fimc_is_sensor1 - XMIPI_CSI4LB */
&fimc_is_sensor1 {
	scenario = <SENSOR_SCENARIO_NORMAL>;		/* Normal, Vision, OIS etc */
	id = <1>;
	csi_ch = <0>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	is_flite = <0>;
	status = "okay";
};

/* fimc_is_sensor2 - XMIPI_CSI4LC */
&fimc_is_sensor2 {
	scenario = <SENSOR_SCENARIO_NORMAL>;		/* Normal, Vision, OIS etc */
	id = <3>;
	csi_ch = <2>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	is_flite = <0>;
	status = "disabled";
};

/* fimc_is_sensor3 - XMIPI_CSI2L */
&fimc_is_sensor3 {
	scenario = <SENSOR_SCENARIO_NORMAL>;		/* Normal, Vision, OIS etc */
	id = <2>;
	csi_ch = <3>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	is_flite = <0>;
	status = "disabled";
};

/* hsi2c_0: hsi2c@138A0000 */
&hsi2c_0 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc1 0 0 &gpc1 1 0>;
	status = "okay";
	clock-frequency = <400000>;

	samsung,scl-extended-low-period;
	samsung,reset-before-trans;

	pinctrl-names = "on_i2c";
	pinctrl-0 = <&hs_i2c0_bus>;

	fimc-is-5e9@5A {
		compatible = "samsung,exynos5-fimc-is-cis-5e9";
		reg = <0x2D>; 				/* 1 bit right shift */
		id = <1>;				/* matching sensor id */
		sensor_f_number = <200>; 		/* f number 2.0 */
		setfile = "setB";
		use_sensor_otp;				/* USE OTP CAL MAP */
		rom_position = <1>;			/* SENSOR_POSITION_REAR2 */
		use_initial_ae;				/* for initial AE */
	};
};

/* hsi2c_1: hsi2c@138B0000 */
&hsi2c_1 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc1 2 0 &gpc1 3 0>;
	status = "okay";
	clock-frequency = <400000>;

	samsung,scl-extended-low-period;
	samsung,reset-before-trans;

	pinctrl-names = "on_i2c";
	pinctrl-0 = <&hs_i2c1_bus>;
	fimc-is-3l6@5A {
		compatible = "samsung,exynos5-fimc-is-cis-3l6";
		reg = <0x2D>;				/* 1 bit right shift */
		id = <0>;				/* matching sensor id */
		sensor_f_number = <190>;		/* f number 1.9 */
		setfile = "setB";
	};
};

/* hsi2c_2: hsi2c@138C0000 */
&hsi2c_2 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc1 4 0 &gpc1 5 0>;
	status = "okay";
	clock-frequency = <400000>;

	samsung,scl-extended-low-period;
	samsung,reset-before-trans;

	pinctrl-names = "on_i2c";
	pinctrl-0 = <&hs_i2c2_bus>;
};

/* hsi2c_3: hsi2c@138D0000 */
&hsi2c_3 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc1 6 0 &gpc1 7 0>;
	status = "disabled";
	clock-frequency = <400000>;

	samsung,scl-extended-low-period;
	samsung,reset-before-trans;

	pinctrl-names = "on_i2c";
	pinctrl-0 = <&hs_i2c3_bus>;

	fimc-is-actuator@18 {
		compatible = "samsung,exynos5-fimc-is-actuator-dw9808";
		reg = <0x0C>;
		id = <0>;
		place = <0>;
	};

	rear_eeprom@B0 {
		compatible = "samsung,rear-eeprom-i2c";
		reg = <0x58>;		/* 7bit-addr */
	};
};

&i2c_1{
		status = "okay";
		s2mu005@3D {
			status = "okay";
			compatible = "samsung,s2mu005mfd";
			reg = <0x3D>;
			pinctrl-names = "default";
			pinctrl-0 = <&if_pmic_irq>;
			s2mu005,irq-gpio = <&gpa2 2 1>;
			s2mu005,wakeup;

			leds {
				flash-gpio = <&gpg2 7 0>;	/* XGPIO19 */
				torch-gpio = <&gpg0 1 0>;	/* XPWMTOUT_0 */

				flash_current = <1200>;
				preflash_current = <175>;
				movie_current = <175>; /*unused*/
				torch_current = <75>;
				factory_current = <150>;
				//front_torch_current = <300>;
				flashlight_current = <25 75 100 150 200>;

				status = "okay";
				s2mu005-leds1 {
					ledname = "leds-sec1";
					id = <0>;
					timeout = <15>; /* use gpio */
				};

				s2mu005-leds2 {
					ledname = "leds-sec2";
					id = <1>;
					timeout = <15>; /* use gpio */
				};
			};
		};
	};

