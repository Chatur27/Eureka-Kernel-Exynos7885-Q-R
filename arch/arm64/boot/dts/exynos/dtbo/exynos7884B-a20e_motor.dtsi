/ {
	fragment@motor {
		target-path = "/";
		__overlay__ {
			/* VIBRATOR */
			sec_vib {
				compatible = "sec_vib";
				sec_vib_pdata {
					status = "okay";
					sec_vib,max_timeout = <10000>;
					sec_vib,ldo_type = <1>;
					sec_vib,ldo_en = <&gpp0 3 1>;
				};
			};
		};
	};
};