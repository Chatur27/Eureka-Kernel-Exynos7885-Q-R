/dts-v1/;
/plugin/;

#include "exynos7884B-a10_common.dtsi"
#include "exynos7884B-a10_sea_open_gpio_00.dtsi"
/ {
	compatible = "samsung,A10 SEA OPEN 00", "samsung,EXYNOS7884B";

	dtbo-hw_rev = <0>;
    dtbo-hw_rev_end = <255>;


	fragment@model {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;

			model = "Samsung A10 SEA OPEN 00 board based on EXYNOS7884B";

		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

&i2c_4 {
	mms_ts@48 {
		status = "okay";
	};
};