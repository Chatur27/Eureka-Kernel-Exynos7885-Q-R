	sysmmu_mfcmscl: sysmmu@12C50000 {
		compatible = "samsung,exynos-sysmmu";
		reg = <0x0 0x12C50000 0x9000>;
		interrupts = <0 198 0>, <0 199 0>;
		qos = <15>;
		clock-names = "aclk";
		clocks = <&clock GATE_SMMU_MFCMSCL>;
		port-name = "G2D, SMFC, Scaler, MFC";
		sysmmu,secure-irq;
		sysmmu,secure_base = <0x12C60000>;
		sysmmu,tlb_property =
			<(SYSMMU_PORT_NO_PREFETCH_READ(0x0)		| SYSMMU_BL1) SYSMMU_NOID>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL2) SYSMMU_ID_MASK(0x0, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READ(0x1)		| SYSMMU_BL4) SYSMMU_ID_MASK(0x1, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READ(0x1)		| SYSMMU_BL1) SYSMMU_ID_MASK(0x5, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READ(0x1)		| SYSMMU_BL1) SYSMMU_ID_MASK(0x9, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_WRITE(0x1)		| SYSMMU_BL4) SYSMMU_ID_MASK(0x21, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_WRITE(0x1)		| SYSMMU_BL1) SYSMMU_ID_MASK(0x25, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_WRITE(0x1)		| SYSMMU_BL1) SYSMMU_ID_MASK(0x29, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL16) SYSMMU_ID_MASK(0x2, 0xFFFF)>,
			<(SYSMMU_PORT_NO_PREFETCH_READWRITE(0x1)	| SYSMMU_BL8)  SYSMMU_ID_MASK(0x3, 0xFFFF)>;
		#iommu-cells = <0>;
	};

